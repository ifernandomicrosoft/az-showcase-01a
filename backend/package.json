{
  "name": "bankgpt-backend",
  "version": "1.0.0",
  "description": "Backend service for BankGPT with OpenAI integration",
  "main": "src/app.js",
  "scripts": {
    "start": "node src/app.js",
    "dev": "nodemon src/app.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.js",
    "lint:fix": "eslint src/**/*.js --fix",
    "build": "npm run lint && npm run test",
    "docker:build": "docker build -t bankgpt-backend .",
    "docker:run": "docker run -p 3000:3000 --env-file .env bankgpt-backend"
  },
  "keywords": [
    "banking",
    "chatbot",
    "openai",
    "financial-advisor",
    "api"
  ],
  "author": "BankGPT Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "openai": "^4.20.1",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "dotenv": "^16.3.1",
    "express-rate-limit": "^7.1.5",
    "joi": "^17.11.0",
    "winston": "^3.11.0",
    "redis": "^4.6.10",
    "mongodb": "^6.3.0",
    "uuid": "^9.0.1",
    "compression": "^1.7.4",
    "express-validator": "^7.0.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "eslint": "^8.56.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "@types/jest": "^29.5.8"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ifernandomicrosoft/az-showcase-01a.git"
  },
  "bugs": {
    "url": "https://github.com/ifernandomicrosoft/az-showcase-01a/issues"
  },
  "homepage": "https://github.com/ifernandomicrosoft/az-showcase-01a#readme"
}